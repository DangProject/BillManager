      </EndProperty>
    </AssociationSetMapping>
    <AssociationSetMapping Name="AccountWebsite" TypeName="BillManagerModel.AccountWebsite" StoreEntitySet="Websites">
      <EndProperty Name="Account">
        <ScalarProperty Name="AccountId" ColumnName="AccountWebsite_Website_AccountId" />
      </EndProperty>
      <EndProperty Name="Website">
        <ScalarProperty Name="WebsiteId" ColumnName="WebsiteId" />
      </EndProperty>
    </AssociationSetMapping>
