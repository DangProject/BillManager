<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--Foreground-->
    <SolidColorBrush x:Key="ForegroundColor" Color="#FFFFFF"/>

    <!--General-->
    <SolidColorBrush x:Key="BackgroundHighlighted" Color="#54545C"/>
    <SolidColorBrush x:Key="BorderBrushHighlighted" Color="#6A6A75"/>
    <SolidColorBrush x:Key="BackgroundSelected" Color="#007ACC"/><!--#FF91CEF7#FF9DD3F7-->
    <SolidColorBrush x:Key="BackgroundLightBlueSelected" Color="#FF91CEF7"/>
    <SolidColorBrush x:Key="BorderBrushSelected" Color="#1C97EA"/>
    <SolidColorBrush x:Key="BackgroundNormal" Color="#3F3F46"/>
    <SolidColorBrush x:Key="BorderBrushNormal" Color="#54545C"/>

    <!--Scroll Bar-->    
    <SolidColorBrush x:Key="ScrollBarPageButtonBackgroundHighlighted" Color="#05FFFFFF"/>
    
    <SolidColorBrush x:Key="IntelBlueBrush" Color="#0071C5"/>
    <SolidColorBrush x:Key="IntelLightBlueBrush" Color="#00AEEF"/>
    <SolidColorBrush x:Key="IntelDarkBlueBrush" Color="#004280"/>
    <SolidColorBrush x:Key="BlueBackgroundBrush" Color="#FF0E6DB4"/>
