    <LinearGradientBrush x:Key="SliderOnBorderBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF022641" Offset="0" />
        <GradientStop Color="#FF0696CC" Offset="1" />
        <GradientStop Color="#FF0477A6" Offset="0.586"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="SliderOnBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF033356" Offset="0" />
        <GradientStop Color="#FF04B8FB" Offset="1" />
        <GradientStop Color="#FF038FC9" Offset="0.20"/>
    </LinearGradientBrush>

    <!--<LinearGradientBrush x:Key="SliderOnBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
