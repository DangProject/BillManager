        <GradientStop Color="#FF016192" Offset="0.15"/>
    </LinearGradientBrush>-->
    
    <LinearGradientBrush x:Key="SliderOffBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF5E5E5E" Offset="0" />
        <GradientStop Color="#FFFEFEFE" Offset="1" />
        <GradientStop Color="#FFB6B6B6" Offset="0.20"/>
    </LinearGradientBrush>

