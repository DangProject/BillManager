    <SolidColorBrush x:Key="IntelDarkYellowBrush" Color="#FDBB13"/>
    <SolidColorBrush x:Key="IntelGreenBrush" Color="#A6CE39"/>
    <SolidColorBrush x:Key="IntelGrayBrush" Color="#939598"/>
