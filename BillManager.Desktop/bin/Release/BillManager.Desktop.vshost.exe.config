<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://DANGER-SERVER:8009/AccountService"
                binding="netTcpBinding"
                contract="BillManager.Client.ServiceContracts.IAccountService" />
      <endpoint address="net.tcp://DANGER-SERVER:8009/PaymentService"
                binding="netTcpBinding"
                contract="BillManager.Client.ServiceContracts.IPaymentService"  />
    </client>
    <bindings>
      <netTcpBinding>
        <binding maxReceivedMessageSize="2147483647"
                 maxBufferPoolSize="2147483647"
                 maxBufferSize="2147483647"
                 transactionFlow="false"
                 openTimeout="00:05:00"
                 sendTimeout="00:05:00"
                 receiveTimeout="00:10:00" >
          <reliableSession enabled="false"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

